<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity GDPRMyAnswer.
    -->
    <changeSet id="20190924115810-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="questionnaire_status_id"
                                 baseTableName="gdpr_my_answer"
                                 constraintName="fk_gdprmy_answer_questionnaire_status_id"
                                 referencedColumnNames="id"
                                 referencedTableName="gdpr_questionnaire_status"/>

        <addForeignKeyConstraint baseColumnNames="question_id"
                                 baseTableName="gdpr_my_answer"
                                 constraintName="fk_gdprmy_answer_question_id"
                                 referencedColumnNames="id"
                                 referencedTableName="gdpr_question"/>

        <addForeignKeyConstraint baseColumnNames="answer_id"
                                 baseTableName="gdpr_my_answer"
                                 constraintName="fk_gdprmy_answer_answer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="gdpr_answer"/>

    </changeSet>
</databaseChangeLog>
