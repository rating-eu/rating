<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity GDPRQuestionnaireStatus.
    -->
    <changeSet id="20190924115807-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="operation_id"
                                 baseTableName="gdpr_questionnaire_status"
                                 constraintName="fk_gdprquestionnaire_status_operation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="data_operation"/>

        <addForeignKeyConstraint baseColumnNames="questionnaire_id"
                                 baseTableName="gdpr_questionnaire_status"
                                 constraintName="fk_gdprquestionnaire_status_questionnaire_id"
                                 referencedColumnNames="id"
                                 referencedTableName="gdpr_questionnaire"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="gdpr_questionnaire_status"
                                 constraintName="fk_gdprquestionnaire_status_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
