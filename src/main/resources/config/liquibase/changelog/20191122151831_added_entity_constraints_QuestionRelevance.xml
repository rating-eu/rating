<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity QuestionRelevance.
    -->
    <changeSet id="20191122151831-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="question_id"
                                 baseTableName="question_relevance"
                                 constraintName="fk_question_relevance_question_id"
                                 referencedColumnNames="id"
                                 referencedTableName="question"/>

        <addForeignKeyConstraint baseColumnNames="status_id"
                                 baseTableName="question_relevance"
                                 constraintName="fk_question_relevance_status_id"
                                 referencedColumnNames="id"
                                 referencedTableName="questionnaire_status"/>

    </changeSet>
</databaseChangeLog>
